{
  "runtime": {
    "version": "LuaJIT",
    "requirePattern": [
      "lua/?.lua",
      "lua/?/init.lua",
      "?/lua/?.lua",
      "?/lua/?/init.lua"
    ]
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "$HOME/build/LLS-Addons/addons/luvit",
      "$NIXCATS_VIM_PACK_DIR"
    ],
    "ignoreGlobs": ["**/*_spec.lua"]
  }
}
